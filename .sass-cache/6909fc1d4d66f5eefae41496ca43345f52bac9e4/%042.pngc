// All sprites should extend this class
// The Clusters-sprite mixin will do so for you.
@if $Clusters-inline {
  #{$Clusters-sprite-base-class} {
    background-image: inline-sprite($Clusters-sprites);
  } 
} @else {
  #{$Clusters-sprite-base-class} {
    background: $Clusters-sprites no-repeat;
  }
}
//sass functions to return the dimensions of a sprite image as units

  @function Clusters-sprite-width($name) {
    $file: sprite_file($Clusters-sprites, $name);
    @return image-width($file);
  }

  @function Clusters-sprite-height($name) {
    $file: sprite_file($Clusters-sprites, $name);
    @return image-height($file);
  }


// Use this to set the dimensions of an element
// based on the size of the original image.
@mixin Clusters-sprite-dimensions($name) {
  @include sprite-dimensions($Clusters-sprites, $name)
}

// Move the background position to display the sprite.
@mixin Clusters-sprite-position($name, $offset-x: 0, $offset-y: 0) {
  @include sprite-background-position($Clusters-sprites, $name, $offset-x, $offset-y)
}

// Extends the sprite base class and set the background position for the desired sprite.
// It will also apply the image dimensions if $dimensions is true.
@mixin Clusters-sprite($name, $dimensions: $Clusters-sprite-dimensions, $offset-x: 0, $offset-y: 0) {
  @extend #{$Clusters-sprite-base-class};
  @include sprite($Clusters-sprites, $name, $dimensions, $offset-x, $offset-y)
}

@mixin Clusters-sprites($sprite-names, $dimensions: $Clusters-sprite-dimensions, $prefix: sprite-map-name($Clusters-sprites), $offset-x: 0, $offset-y: 0) {
  @include sprites($Clusters-sprites, $sprite-names, $Clusters-sprite-base-class, $dimensions, $prefix, $offset-x, $offset-y)
}

// Generates a class for each sprited image.
@mixin all-Clusters-sprites($dimensions: $Clusters-sprite-dimensions, $prefix: sprite-map-name($Clusters-sprites), $offset-x: 0, $offset-y: 0) {
  @include Clusters-sprites(cccm coordination early-recovery education emergency-telecommunications food-security grey-s8e6e32aa57 health logistics nutrition protection shelters wash, $dimensions, $prefix, $offset-x, $offset-y);
